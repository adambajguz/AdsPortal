{
  "Logging": {
    "FilePath": "../logs/",
    "FileNameTemplate": "production_.log",
    "RetainedFileCountLimit": 64,
    "FileSizeLimitInBytes": 10485760,
    "FlushIntervalInSeconds": 5,
    "IsFileOutputEnabled": true,
    "FileOutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{SourceContext}] <{ProcessId}:{ThreadId}> ({RequestId} {RequestPath}) {Message:lj}{NewLine}{Exception}",
    "IsConsoleOutputEnabled": true,
    "ConsoleOutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] [{SourceContext}] <{ThreadId}> ({RequestId} {RequestPath}) {Message:lj}{NewLine}{Exception}",
    "LogEverythingInDev": false,
    "SentryDSN": "",
    "SentryEnabled": false
  },
  "AllowedHosts": "*"
}
