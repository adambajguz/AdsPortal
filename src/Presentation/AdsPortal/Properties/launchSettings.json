{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "AdsPortal (Development)": {
      "commandName": "Project",
      "workingDirectory": "$(TargetDir)",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "AdsPortal (Development) Interactive": {
      "commandName": "Project",
      "commandLineArgs": "interactive",
      "workingDirectory": "$(TargetDir)",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "AdsPortal (Development) --help": {
      "commandName": "Project",
      "commandLineArgs": "--help",
      "workingDirectory": "$(TargetDir)",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "AdsPortal (Production)": {
      "commandName": "Project",
      "workingDirectory": "$(TargetDir)",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "AdsPortal (Production) Interactive": {
      "commandName": "Project",
      "commandLineArgs": "interactive",
      "workingDirectory": "$(TargetDir)",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "AdsPortal (Production) --help": {
      "commandName": "Project",
      "commandLineArgs": "--help",
      "workingDirectory": "$(TargetDir)",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    }
  }
}