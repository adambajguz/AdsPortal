{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "AdsPortal (Development)": {
      "commandName": "Project",
      "launchBrowser": true,
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:2138;http://localhost:2137"
    },
    "AdsPortal (Development) [Interactive]": {
      "commandName": "Project",
      "commandLineArgs": "[interactive]",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:2138;http://localhost:2137"
    },
    "AdsPortal (Development) --help": {
      "commandName": "Project",
      "commandLineArgs": "--help",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "AdsPortal (Production)": {
      "commandName": "Project",
      "launchBrowser": true,
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "http://localhost:2138;http://localhost:2137"
    },
    "AdsPortal (Production) [Interactive]": {
      "commandName": "Project",
      "commandLineArgs": "[interactive]",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "http://localhost:2138;http://localhost:2137"
    },
    "AdsPortal (Production) --help": {
      "commandName": "Project",
      "commandLineArgs": "--help",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    }
  }
}