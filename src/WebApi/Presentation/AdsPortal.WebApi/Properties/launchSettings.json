{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "AdsPortal.WebApi (Development)": {
      "commandName": "Project",
      "workingDirectory": "$(TargetDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5002/api/index.html",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5002"
    },
    "AdsPortal.WebApi (Production)": {
      "commandName": "Project",
      "workingDirectory": "$(TargetDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5002/api/index.html",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "https://localhost:5002"
    },
    "AdsPortal.WebApi (Test)": {
      "commandName": "Project",
      "workingDirectory": "$(TargetDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5002/api/index.html",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Test"
      },
      "applicationUrl": "https://localhost:5002"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/index.html",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}